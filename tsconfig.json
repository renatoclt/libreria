{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "ngx-rclt-cards": [
        "dist/ngx-rclt-cards"
      ],
      "ngx-rclt-cards/*": [
        "dist/ngx-rclt-cards/*"
      ],
      "ngx-rclt": [
        "dist/ngx-rclt",
        "dist/ngx-rclt"
      ],
      "ngx-rclt/*": [
        "dist/ngx-rclt/*",
        "dist/ngx-rclt/*"
      ],
      "ngx-util": [
        "dist/ngx-util"
      ],
      "ngx-util/*": [
        "dist/ngx-util/*"
      ],
      "ngx-util-utilitarios": [
        "dist/ngx-util-utilitarios"
      ],
      "ngx-util-utilitarios/*": [
        "dist/ngx-util-utilitarios/*"
      ],
      "ngx-rclt-utilitarios": [
        "dist/ngx-rclt-utilitarios"
      ],
      "ngx-rclt-utilitarios/*": [
        "dist/ngx-rclt-utilitarios/*"
      ],
      "ngx-common": [
        "dist/ngx-common",
        "dist/ngx-common"
      ],
      "ngx-common/*": [
        "dist/ngx-common/*",
        "dist/ngx-common/*"
      ],
      "ngx-utilitarioclt": [
        "dist/ngx-utilitarioclt"
      ],
      "ngx-utilitarioclt/*": [
        "dist/ngx-utilitarioclt/*"
      ],
      "ngx-utilitario-rclt": [
        "dist/ngx-utilitario-rclt"
      ],
      "ngx-utilitario-rclt/*": [
        "dist/ngx-utilitario-rclt/*"
      ]
    }
  }
}